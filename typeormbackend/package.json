      "bl": "^3.0.0",
      "bluebird": "^3.5.5",
      "body-parser": "^1.19.0",
      "config": "^1.26.1",
      "cors": "^2.8.1",
      "dateformat": "^2.0.0",
      "express": "^4.17.1",
      "express-jwt": "^5.1.0",
      "finnish-bank-utils": "^1.3.3",
      "ibantools": "^1.3.0",
