{
  "name": "server",
  "version": "1.0.0",
  "description": "IlmoV2 server",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node bin/start.js",
    "watch-node": "nodemon bin/start.js --watch src/* -e ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json --project tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect bin/start.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venttola/IlmoV2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/venttola/IlmoV2/issues"
  },
  "homepage": "https://github.com/venttola/IlmoV2#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bl": "^3.0.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "config": "^1.26.1",
    "cors": "^2.8.1",
    "dateformat": "^2.0.0",
    "express": "^4.17.1",
    "express-jwt": "^5.1.0",
    "finnish-bank-utils": "^1.3.3",
    "ibantools": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mysql": "^2.17.1",
    "nodemailer": "^4.6.4",
    "nodemailer-express-handlebars": "^3.0.0",
    "orm": "^5.0.4",
    "postman": "^0.2.0",
    "semver": "^6.1.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.32",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.34",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/node": "^6.14.6",
    "@types/nodemailer": "^4.6.8",
    "@types/serve-static": "^1.7.31",
    "@types/underscore": "^1.8.18",
    "faker": "^4.1.0",
    "migrate-orm2": "^4.0.5",
    "nodemon": "^1.19.1",
    "promise": "^7.1.1",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.4.1",
    "rollup-plugin-uglify": "^1.0.2",
    "tslib": "^1.7.0",
    "tslint": "^3.15.1",
    "typescript": "^2.8.3"
  },
  "apidoc": {
    "title": "IlmoV2 API V 1",
    "filename": "README.md",
    "url": "localhost:4200/"
  }
}
